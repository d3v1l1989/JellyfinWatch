# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token
DISCORD_AUTHORIZED_USERS=user1,user2

# Jellyfin Server Configuration
JELLYFIN_URL=http://your-jellyfin-server:8096
JELLYFIN_API_KEY=your_jellyfin_api_key
# Alternative authentication (use either API key or username/password)
JELLYFIN_USERNAME=your_jellyfin_username
JELLYFIN_PASSWORD=your_jellyfin_password

# Discord Channel Configuration
CHANNEL_ID=your_discord_channel_id

# Optional: SABnzbd Configuration
SABNZBD_URL=http://192.168.1.1:8282
SABNZBD_API_KEY=your_sabnzbd_api_key

# Optional: Uptime Kuma Configuration
UPTIME_URL=http://192.168.1.1:3001
UPTIME_USERNAME=your_kuma_username
UPTIME_PASSWORD=your_kuma_password
UPTIME_MONITOR_ID=your_monitor_id

# Docker Configuration
RUNNING_IN_DOCKER=false